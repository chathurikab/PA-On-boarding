      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
SELECT p.Id AS 'Property ID'
FROM dbo.OwnerProperty op INNER JOIN dbo.Property p ON op.PropertyId =p.Id
WHERE p.IsActive =1</CommandText>
